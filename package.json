{
  "name": "medtrust",
  "version": "0.1.0",
  "description": "AI-Powered Healthcare Intelligence Platform for India",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "start:local": "ENABLE_CLOUD_MODE=false react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest --testPathPattern=property",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "detox test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace MedTrust.xcworkspace -scheme MedTrust -configuration Release",
    "deploy:aws": "cd aws && cdk deploy"
  },
  "keywords": [
    "healthcare",
    "ai",
    "prescription",
    "medical",
    "india",
    "aws",
    "bedrock",
    "textract"
  ],
  "author": "HAI - Health AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mjpvl-ai/MedTrust.git"
  },
  "bugs": {
    "url": "https://github.com/mjpvl-ai/MedTrust/issues"
  },
  "homepage": "https://github.com/mjpvl-ai/MedTrust#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "react-redux": "^8.1.0",
    "redux": "^4.2.1",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "react-native-camera": "^4.2.1",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-voice": "^3.2.4",
    "i18next": "^23.0.0",
    "react-i18next": "^13.0.0",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.0",
    "jest": "^29.6.0",
    "@testing-library/react-native": "^12.2.0",
    "@testing-library/jest-native": "^5.4.0",
    "fast-check": "^3.12.0",
    "detox": "^20.11.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
